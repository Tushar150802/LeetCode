class Solution {
    public List<Integer> preorderTraversal(TreeNode root) {
    preOrder(root);
    return res;        
    }
    private List<Integer> res=new ArrayList<>();
    private void preOrder(TreeNode node)
    {
        if(node==null){
            return;
        }
        res.add(node.val);
        preOrder(node.left);
        preOrder(node.right);
    }
}
